          ],
          [
            {"type": "header", "value": "Proper names"},
            {"type": "gramm", "value": "persn", "category": "case", "tooltip": "Name"},
            {"type": "gramm", "value": "patrn", "category": "case", "tooltip": "Patronymic"},
            {"type": "gramm", "value": "famn", "category": "case", "tooltip": "Family name"},
